# Docker Compose variables
DB_NAME=sketchymine_db
DB_USER=sketchymine
DB_PASS=smdbpass

# Node server DB connection string
DB_URL=postgres://sketchymine:smdbpass@localhost:5432/sketchymine_db

# Sync DB Schema
SYNC=false

# Sentry error logging DSN
#SENTRY_DSN=

# Game options
ROUND_DURATION=8
ROUND_POINTS_MULTIPLIER=1
EMERALD_SHARD_VALUE=5
LEVEL_SHARDS=1

# File with initial words
WORDBANK=words.db

# Authentication
#GITLAB_APP_ID=
#GITLAB_APP_SECRET=

CLIENT_URL=http://localhost:8080
API_URL=http://localhost:3000